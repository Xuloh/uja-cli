{
  "name": "uja-cli",
  "version": "0.1.0",
  "description": "CLI wrapper for the unofficial jisho api package",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --mode production --progress",
    "postbuild": "chmod +x build/uja",
    "prebuild-exe": "npm run build",
    "build-exe": "pkg build/uja --targets latest-linux-x64,latest-win-x64 --out-path build",
  },
  "author": "Xuloh",
  "license": "MIT",
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "color-support": "^1.1.3",
    "unofficial-jisho-api": "^2.0.13",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "pkg": "^4.4.9",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0"
  }
}
